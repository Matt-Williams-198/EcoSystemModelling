InitialPredatorPopulations = 2;
InitialPreyPopulations = [30, 50];
TimeStep = 1;
FinalTime = 100;
CurrentTime = linspace(0.0,FinalTime,FinalTime/TimeStep);
CurrentTime = fix(CurrentTime);PreyGrowthRates = [2, 2];
PredatorGrowthRates = [1, 1];
RemovalInterval = 10;
RemovalProportions = 0.4;
Predation1Rates = 1;
Predation2Rates = 1;
PreyCarryingCapacities = [100, 100];
PreyCompetitionCoefficients = [0.1, 0.1];
PredatorCompetitionCoefficients = 0.0;
PredatorDeathRates = 0.2;
TimeEvolvedPopulations = zeros(3,FinalTime);
TimeEvolvedPopulations(1,1) = InitialPreyPopulations(1);
TimeEvolvedPopulations(2,1) = InitialPreyPopulations(2);
TimeEvolvedPopulations(3,1) = InitialPredatorPopulations;
for i = 2 : FinalTime
    [TimeEvolvedPopulations(:,i)] = TimeEvolvedPredatorPreySystem( ...
                                    [TimeEvolvedPopulations(1,i-1),TimeEvolvedPopulations(2,i-1)], ...
                                    InitialPredatorPopulations, ...
                                    TimeEvolvedPopulations(3,i-1), ...
                                    TimeStep, ...
                                    CurrentTime(i), ...
                                    PreyGrowthRates, ...
                                    PredatorGrowthRates, ...
                                    RemovalInterval, ...
                                    RemovalProportions, ...
                                    Predation1Rates, ...
                                    Predation2Rates, ...
                                    PreyCarryingCapacities, ...
                                    PreyCompetitionCoefficients,...
                                    PredatorCompetitionCoefficients,...
                                    PredatorDeathRates);
    mod(CurrentTime(i),RemovalInterval)
end
[t,y] = ode45(@(t,y) TimeEvolvedPredatorPreySystem( ...
                                    [TimeEvolvedPopulations(1,i-1),TimeEvolvedPopulations(2,i-1)], ...
                                    InitialPredatorPopulations, ...
                                    TimeEvolvedPopulations(3,i-1), ...
                                    TimeStep, ...
                                    CurrentTime, ...
                                    PreyGrowthRates, ...
                                    PredatorGrowthRates, ...
                                    RemovalInterval, ...
                                    RemovalProportions, ...
                                    Predation1Rates, ...
                                    Predation2Rates, ...
                                    PreyCarryingCapacities, ...
                                    PreyCompetitionCoefficients,...
                                    PredatorCompetitionCoefficients,...
                                    PredatorDeathRates),[0 100],[100,50,1]);
XData = linspace(1,FinalTime,100);
figure
plot(XData,TimeEvolvedPopulations(1,:))
hold on
plot(XData,TimeEvolvedPopulations(2,:))
plot(XData,TimeEvolvedPopulations(3,:))